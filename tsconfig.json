{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,   
      "esModuleInterop": true,
      "strictNullChecks": false,
      "skipLibCheck": true,    
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   }
}
